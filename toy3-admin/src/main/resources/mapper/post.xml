<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.fastcampus.toy3admin.model.post.PostRepository">

    <select id="findAll" resultType="com.fastcampus.toy3admin.model.post.Post">
        SELECT * FROM post_tb
    </select>

    <delete id="deleteById" parameterType="String">
        DELETE FROM post_tb WHERE id = #{id}
    </delete>

</mapper>